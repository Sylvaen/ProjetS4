<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.Saccoche" table="saccoche">
		<id name="id" column="id" type="int">
			<generator class="native"></generator>
		</id>
		<property name="idparties" column="idpartie" type="integer"></property>
		<property name="listLettre" column="contenu" type="string"></property>
		<property name="nbLettres" column="nblettre" type="integer"></property>
	</class>
</hibernate-mapping>