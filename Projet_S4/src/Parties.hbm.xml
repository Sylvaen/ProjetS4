<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.Parties" table="parties">
		<id name="id" column="id" type="int">
			<generator class="native"></generator>
		</id>
		<property name="nom" column="name" type="string"></property>
		<property name="idj1" column="joueur1" type="integer"></property>
		<property name="idj2" column="joueur2" type="integer"></property>
		<property name="plateauString" column="plateau" type="string"></property>
	</class>
</hibernate-mapping>